<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\firely-serverless-validator.props" />

  <PropertyGroup>
    <Title>Firely validation library</Title>
    <Description>Next-gen Firely validation library for FHIR data.</Description>
    <PackageTags>HL7;FHIR;Validation;Utility;</PackageTags>
    <AssemblyName>Firely.Fhir.Validation</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Hl7.Fhir.Serialization" Version="$(FhirNetApiCommonVersion)" />
    <PackageReference Include="Hl7.Fhir.ElementModel" Version="$(FhirNetApiCommonVersion)" />
    <PackageReference Include="Hl7.Fhir.Support.Poco" Version="$(FhirNetApiCommonVersion)" />
    <PackageReference Include="Hl7.FhirPath" Version="$(FhirNetApiCommonVersion)" />
    <PackageReference Include="MessagePack.Annotations" Version="2.2.85" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

</Project>
